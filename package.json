{
  "name": "tree-sitter-lean",
  "version": "0.1.0",
  "description": "Bindings for a tree sitter grammar for the Lean theorem prover",
  "repository": "github:Julian/tree-sitter-lean",
  "license": "MIT",
  "author": {
    "name": "Julian Berman",
    "email": "Julian+tree-sitter-lean@GrayVines.com"
  },
  "packageManager": "pnpm@9.15.1",
  "type": "module",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "lean"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.3.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.24.6"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "tree-sitter": [
    {
      "scope": "source.lean",
      "injection-regex": "^lean$"
    }
  ]
}
